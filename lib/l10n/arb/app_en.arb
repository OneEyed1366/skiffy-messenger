    "@homeserverUrlPlaceholder": {
        "description": "Placeholder text for homeserver URL input field"
    },
    "homeserverUrlHint": "Enter your Matrix server address",
    "@homeserverUrlHint": {
        "description": "Hint text for homeserver URL input field"
