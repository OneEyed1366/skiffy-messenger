{
    "@@locale": "en",
    "counterAppBarTitle": "SkiffyMessenger",
    "@counterAppBarTitle": {
        "description": "Text shown in the AppBar of the Counter Page"
    },
    "textFieldSearchHint": "Search...",
    "@textFieldSearchHint": {
        "description": "Hint text for search text fields"
    },
    "textFieldShowPassword": "Show password",
